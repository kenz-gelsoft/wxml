      <memberdef kind="function" id="classwx_data_view_check_icon_text_renderer_1a0f7220ef04df5866162a907da15b5f14" prot="public" static="no" const="no" explicit="no" inline="no" virt="non-virtual">
        <type>void</type>
        <definition>void wxDataViewCheckIconTextRenderer::Allow3rdStateForUser</definition>
        <argsstring>(bool allow=true)</argsstring>
